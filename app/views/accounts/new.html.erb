        <%= form_for @account do |f| %>
          <%= f.label :f_name %></br>
          <%= f.text_field :f_name, placeholder: "fname" %></dr>

          <%= f.label :t_name %></br>
          <%= f.text_field :t_name, placeholder: "tname" %></dr>

          <%= f.label :yourImage %></br>
          <%= f.file_field :image %></br>

          <%= f.label :profile %></br>
          <%= f.text_field :profile, placeholder: "profile" %></dr>

          <%= f.label :link %></br>
          <%= f.text_field :link, placeholder: "link" %></dr>

          <%= f.submit %>
        <% end %>
