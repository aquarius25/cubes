<div class="wrapper">
  <header>
    <div class="header-top">
      <div class="header-top__left">
        <p class="title">User</p>
      </div>
      <div class="header-top__right">
      </div>
    </div>
    <div class="header-bottom">
      <%= link_to users_path do %>
        <i class="fas fa-user-alt icon color"></i>
      <% end %>
      <%= link_to tweets_path do %>
        <i class="far fa-comment-dots icon"></i>
      <% end %>
      <%= link_to groups_path do %>
        <i class="far fa-object-group icon"></i>
      <% end  %>
      <%= link_to articles_path do %>
        <i class="far fa-sticky-note icon"></i>
      <% end %>
    </div>
  </header>
  <main>
    <div class="main-contents">
      <%= render partial: "/shared/left_contents" %>
      <div class="right-contents scroll-container">
        <div class="selfback-image">
          <p class="self-name">Hara Aqua</p>
          <div class="background-image" style="background-image: url(http://acusd1.org/High%20School/1516events/Van%20Voorhis%201516/Info%20Pro%2015-16/18%20Braeton/Pictures/My%20Favorite%20Hobby/Slider3.jpg);">
            <%= image_tag "http://www.somecity.tv/images/ballers/main/673_1.jpg", class: "self-icon" %>
          </div>
        </div>
        <p>
          学歴：</br>
          職歴：</br>
          趣味：
        </p>
      </div>
    </div>
  </main>
</div>
